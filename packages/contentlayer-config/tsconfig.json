{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ESNext",
        "skipLibCheck": true,
        "moduleResolution": "Node",
        "noImplicitAny": false,
        "esModuleInterop": true,
        "strict": true,
        "allowSyntheticDefaultImports": true
    },
    "include": ["**/*.ts"],
    "exclude": ["node_modules"]
}
